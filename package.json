{
  "name": "molend-points",
  "version": "0.0.1",
  "description": "Molend Points",
  "scripts": {
    "snapshot:testnet": "ENV=testnet ts-node src/snapshot.ts",
    "snapshot:mainnet": "ENV=mainnet node dist/snapshot.js",
    "server:testnet": "ENV=testnet ts-node src/server.ts",
    "server:mainnet": "ENV=mainnet node dist/server.js",
    "build": "rm -rf dist && tsc",
    "lint": "eslint src/**/*.ts --fix",
    "prettier": "prettier src/**/*.ts --write",
    "prepare": "husky install",
    "release": "yarn build && npx np --no-tests"
  },
  "dependencies": {
    "@urql/core": "^4.1.3",
    "bignumber.js": "^9.1.1",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "ethers": "^6.6.2",
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "sequelize": "^6.29.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  }
}
